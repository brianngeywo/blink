<!DOCTYPE html>
<html>
  <head>
    <title>Blink</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="bg-gray-300">
  <% flash.each do |name, msg| %>
    <div><%= msg %></div>
  <% end %>
    <%= yield %>
  </body>
</html>
